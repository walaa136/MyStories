<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mystory">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.MtStory">
        <activity
            android:name="com.example.mystory.StoryDetailsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.mystory.AddStoryActivity"
            android:exported="false" />
        <activity
            android:name="com.example.mystory.LoginActivity"
            android:exported="false" />
        <activity
            android:name="com.example.mystory.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.mystory.MainActivity"
            android:exported="true" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>